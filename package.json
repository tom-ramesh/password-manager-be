{
  "name": "password-manager-backend",
  "version": "1.0.0",
  "description": "Backend service for password manager",
  "type": "module",
  "main": "dist/index.js",
  "_moduleAliases": {
    "@": "dist",
    "@/models": "dist/models",
    "@/services": "dist/services",
    "@/utils": "dist/utils",
    "@/controllers": "dist/controllers",
    "@/routes": "dist/routes"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm --experimental-specifier-resolution=node dist/index.js",
    "dev": "nodemon --config nodemon.json",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/pg": "^8.11.13",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
